class Solution(object):
    def twoSum(self, numbers, target):  #Time: O(N) and space: O(1)
        l=0
        r=len(numbers)-1

        while l<=r: 
            summ = numbers[l]+numbers[r]
            if summ==target:
                return [l+1, r+1]

            elif summ < target:
                l+=1

            else:
                r-=1

